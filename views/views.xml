<odoo>
  <data>
    <!-- purchase.order inherit tree view -->
    <record id="delivery_note_puchaseorder_view_tree" model="ir.ui.view">
      <field name="name">delivery.note.puchaseorder.view.tree</field>
      <field name="model">purchase.order</field>
      <field name="inherit_id" ref="purchase.purchase_order_tree" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='state']" position="after">
          <field name="picking_count" string="Albaranes" />
          <field name="delivery_state" string="Estado AlbarÃ¡n" />
        </xpath>
      </field>
    </record>
    <!-- stock.pinking inherit form view -->
    <record id="view_id" model="ir.ui.view">
      <field name="name">stock.picking.inherit.view.form</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='origin']" position="after">
          <field name="actual_state" />
        </xpath>
      </field>
    </record>
  </data>
</odoo>